<p id="notice"><%= notice %></p>

<p>
  <strong>Product name:</strong>
  <%= @product_line.Product_Name %>
</p>

<p>
  <strong>Product full name:</strong>
  <%= @product_line.Product_Full_Name %>
</p>

<%= link_to 'Edit', edit_product_line_path(@product_line) %> |
<%= link_to 'Back', product_lines_path %>

<table>
  <thead>
      <tr>
        <th>Material Type ID</th>

        <th colspan="3"></th>
      </tr>
  </thead>
  <tbody>
      <% @product_line.material_types.each do |t| %>
        <tr>
          <td><%= link_to t.Material_Number, material_type_path(t)%></td>

          <th colspan="3"></th>
        </tr>
      <% end %>
  </tbody>
</table>